{
    "rules": {
        "ban": [true, ["_", "extend"],
            ["_", "isNull"],
            ["_", "isDefined"]
        ],
        "class-name": true,
        "comment-format": [false,
            "check-space",
            "check-lowercase"
        ],
        "curly": true,
        "eofline": false,
        "forin": false,
        "indent": [true, 4],
        "interface-name": false,
        "jsdoc-format": true,
        "label-position": true,
        "max-line-length": [true, 140],
        "no-arg": true,
        "no-bitwise": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-string-literal": false,
        "no-trailing-whitespace": false,
        "no-unused-expression": false,
        "one-line": [true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "quotemark": [false],
        "radix": false,
        "semicolon": false,
        "typedef": [false],
        "typedef-whitespace": [true, ["callSignature", "noSpace"],
            ["catchClause", "noSpace"],
            ["indexSignature", "space"]
        ],
        "variable-name": false,
        "whitespace": [false,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}